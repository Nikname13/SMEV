<?xml version="1.0" encoding="UTF-8"?>

<?import com.jfoenix.controls.JFXButton?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.*?>
<AnchorPane xmlns="http://javafx.com/javafx"
            xmlns:fx="http://javafx.com/fxml"
            fx:controller="UI.Area.Controller.AreasController"
            prefHeight="400.0" prefWidth="600.0" fx:id="mAnchorPaneArea">
    <children>
        <AnchorPane AnchorPane.leftAnchor="0" AnchorPane.topAnchor="0" style="-fx-background-color: #ffffff;"
                    prefHeight="55" minHeight="55" prefWidth="250" minWidth="250">
            <children>
                <JFXButton onAction="#onClickAdd" ripplerFill="#40a85f" style="-fx-background-color: #a8f1b3;"
                           mnemonicParsing="false" text="Новый район"
                           AnchorPane.leftAnchor="10" AnchorPane.topAnchor="10" AnchorPane.rightAnchor="10"
                           AnchorPane.bottomAnchor="10"/>
            </children>
        </AnchorPane>
        <AnchorPane AnchorPane.leftAnchor="0" AnchorPane.topAnchor="65" style="-fx-background-color: #ffffff;"
                    prefHeight="55" minHeight="55" prefWidth="250" minWidth="250" AnchorPane.bottomAnchor="0">
            <children>
                <TableView fx:id="tableViewArea" AnchorPane.leftAnchor="10" AnchorPane.topAnchor="10"
                           AnchorPane.rightAnchor="10" AnchorPane.bottomAnchor="10">
                    <columns>
                        <TableColumn fx:id="firstColumn" prefWidth="225.0" text="Наименование"/>
                    </columns>
                </TableView>
            </children>
        </AnchorPane>
    </children>
</AnchorPane>
