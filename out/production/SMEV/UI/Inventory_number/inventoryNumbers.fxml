<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.AnchorPane?>
<AnchorPane xmlns="http://javafx.com/javafx"
            xmlns:fx="http://javafx.com/fxml"
            fx:controller="UI.Inventory_number.Controller.InventoryNumbersController"
            fx:id="mAnchorPaneInventoryNumber">

    <TableView fx:id="tableViewNumber" prefHeight="600.0" prefWidth="500.0">
        <columns>
            <TableColumn fx:id="numberInventoryColumn" prefWidth="100.0" text="Номер"/>
            <TableColumn fx:id="numberSupplyColumn" prefWidth="200.0" text="Номер поставки"/>
            <TableColumn fx:id="groupColumn" prefWidth="150.0" text="Состав оборудования"/>
        </columns>
    </TableView>
    <Button fx:id="buttonAdd" layoutX="350.0" layoutY="23.0" mnemonicParsing="false" onAction="#onClickAdd" prefHeight="32.0" prefWidth="75.0" text="Button" />

</AnchorPane>
