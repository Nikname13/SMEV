<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<AnchorPane xmlns="http://javafx.com/javafx"
            xmlns:fx="http://javafx.com/fxml"
            fx:controller="UI.Provider.Controller.ProvidersController"
            prefHeight="400.0" prefWidth="600.0" fx:id="mAnchorPaneProviders">
    <TableView fx:id="tableViewProvider" prefHeight="400.0" prefWidth="300.0">
        <columns>
            <TableColumn fx:id="firstColumn" prefWidth="100.0" text="Имя"/>
            <TableColumn fx:id="secondColumn" prefWidth="200.0" text="Комментарий"/>
        </columns>
    </TableView>
    <Button fx:id="buttonAdd" layoutX="300.0" layoutY="23.0" mnemonicParsing="false" onAction="#onClickAdd" prefHeight="32.0" prefWidth="75.0" text="Button" />

</AnchorPane>
