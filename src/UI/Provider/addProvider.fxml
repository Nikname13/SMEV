<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextArea?>
<AnchorPane xmlns="http://javafx.com/javafx"
            xmlns:fx="http://javafx.com/fxml"
            fx:controller="UI.Provider.Controller.AddProviderController"
            prefHeight="400.0" prefWidth="600.0">
    <children>
        <TextField fx:id="textFieldName"/>
        <Button layoutX="149.0" mnemonicParsing="false" text="Button" onAction="#onClickAdd"/>
        <TextArea fx:id="textAreaDescription" layoutY="25.0" prefHeight="200.0" prefWidth="200.0" />
    </children>
</AnchorPane>
